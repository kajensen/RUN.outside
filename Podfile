    platform :ios, '10.0'
    pod 'Fabric'
    pod 'Crashlytics'
  	pod 'GoogleMaps'
    pod 'RealmSwift'
    pod 'Alamofire'
    pod 'SwiftyJSON'
    pod 'iRate'
    pod 'MGSwipeTableCell'
    pod 'Charts', :git => 'https://github.com/gGololicic/Charts.git'
end

target "RUN watch Extension" do
    platform :watchos, '2.0'
end
